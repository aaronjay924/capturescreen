<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
   <servlet-name>serv</servlet-name>
   <servlet-class>demo.service2</servlet-class>
  </servlet>
    <servlet>
   <servlet-name>serv2</servlet-name>
   <servlet-class>demo.showImg</servlet-class>
  </servlet>
  <servlet-mapping>
  <url-pattern>service.do</url-pattern>
  <servlet-name>serv</servlet-name>
  </servlet-mapping>
    <servlet-mapping>
  <url-pattern>showImg.do</url-pattern>
  <servlet-name>serv2</servlet-name>
  </servlet-mapping>
</web-app>
